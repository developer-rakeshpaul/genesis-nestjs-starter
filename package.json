{
  "name": "genest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist && npm run mjml",
    "build": "nest build",
    "mjml": "mkdirp ./dist/templates && mjml templates/**/*.mjml -o ./dist/templates",
    "seed": "node dist/seed.js",
    "start": "nest start",
    "start:dev": "env NODE_ENV=development nest start --watch",
    "dev": "npm-run-all mjml start:dev",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main.js",
    "watch": "nodemon --exec \"npm run build && npm run start\" --watch src --ext ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate-entities": "npx typeorm-model-generator -h localhost -d genesis -u genesis -e postgres -o ./.tmp -s public --cf pascal --ce pascal --cp camel --lazy"
  },
  "_moduleAliases": {
    "@app": "dist/app",
    "@modules": "dist/app/modules",
    "@config": "dist/config",
    "@core": "dist/core",
    "@seed": "dist/seed"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.4.1",
    "@nest-middlewares/csurf": "^6.0.0",
