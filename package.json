    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.0",
    "@types/module-alias": "^2.0.0",
    "@types/redis": "^2.8.22",
    "apollo-server": "^2.14.1",
    "apollo-server-fastify": "^2.14.1",
